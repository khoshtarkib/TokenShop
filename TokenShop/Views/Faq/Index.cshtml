@model System.Collections.Generic.List<TokenShop.Models.Post>
@{
    Layout = "~/Views/shared/_layout.cshtml";
}
<main class="page faq-page">
    <section class="clean-block clean-faq dark">
        <div class="container">
            <div class="block-heading">
                <h2 class="text-info">@TokenShop.Common.LanguageManagment.Translate("faq")</h2>
            </div>
            <div class="block-content">

                @for (int i = 0; i < Model.Count; i++)
                {
                <div class="faq-item">
                    <h4 class="question">@Model[i].Subject</h4>
                    <div class="answer">
                        <p>@Model[0].PostContent</p>
                    </div>
                </div>
                }
            </div>
        </div>
    </section>
</main>