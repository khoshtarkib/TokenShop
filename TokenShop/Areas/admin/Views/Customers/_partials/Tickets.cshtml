@model List<TokenShop.Models.Ticket>
<div id="home" class="tab-pane active">
    <div class="user-profile-comment-list">

            @for (int i = 0; i < Model.Count; i++)
            {
                <div class="row profile-comment-mg">
                    <div class="col-lg-1 col-md-2 col-sm-2 col-xs-12 right">
                        <div class="user-profile-comment-img projuct-details-img-tab">
                            <a href="#">
                                <img src="/content/profile/img/notification/6.jpg" alt="">
                            </a>
                        </div>
                    </div>
                    <div class="col-lg-9 col-md-9 col-sm-9 col-xs-12 right">
                        <div class="user-profile-comment-content">
                            <p>@Model[i].Subject </p>
                        </div>
                    </div>
                    <div class="col-lg-2 col-md-2 col-sm-2 col-xs-12 right">
                        <div class="user-profile-comment-content">
                            <div class="row">
                                <div class="col-lg-12">
                                    <div class="comment-date-profile profile-details-name-nn">
                                        <span>@Model[i].StatusDatePersian</span>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            }

    </div>
</div>
