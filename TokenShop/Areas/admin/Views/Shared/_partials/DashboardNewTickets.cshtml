@model System.Collections.Generic.List<TokenShop.Models.Ticket>

<div class="about-sparkline shadow-reset desplay-n-tablet-pro">
    <div class="sparkline-hd">
        <div class="main-spark-hd">
            <h1>تیکتهای پاسخ داده نشده</h1>
        </div>
    </div>
    <div class="sparkline-content timeline-scrollbar">
        <div class="row">
            @for (int i=0; i<Model.Count; i++)
            {
                <div class="col-lg-12">
                    <div class="timeline-hd-main" style="border-bottom: solid 1px #eee;padding-bottom: 5px;margin-bottom: 5px;">
                        <div class="timeline-heading-admin">
                            <a style="
    background: white;
    color: #009688;
    padding: 0;
    float: right;
    margin-left: 5px;
" href="/admin/ticket/display/@Model[i].Id">@Model[i].CustomerName</a>
                            <small><i class="fa fa-map-date"></i>@Model[i].StatusDatePersian</small>
                        </div>
                        <div class="mapcontainer">
                            <div class="map">
                                <span>@Model[i].Subject</span>
                            </div>
                        </div>
                    </div>
                </div>
            }
        </div>
    </div>
</div>