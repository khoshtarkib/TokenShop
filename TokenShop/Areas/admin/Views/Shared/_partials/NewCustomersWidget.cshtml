@model List<TokenShop.Models.Customer>
<div class="sparkline7-list profile-online-mg-t-30 shadow-reset">
    <div class="sparkline7-hd">
        <div class="main-spark7-hd">
            <h1>جدیدترین مشتریان</h1>
        </div>
    </div>
    <div class="sparkline7-graph">
        <div class="user-profile-contact">
            <ul class="profile-contact-menu">
                @for (int i = 0; i < Model.Count; i++)
                {
                    <li>
                        <a href="#">
                            <img src="/content/admin/img/notification/1.jpg" alt="" /> <span>@Model[i].FirstName @Model[i].LastName</span> <span class="contact-profile-online-f">@Model[i].RegisterDatePersian</span>
                        </a>
                    </li>
                }
                </ul>
            </div>
        </div>
    </div>